{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.png",
          "/index.csr.html",
          "/index.html",
          "/manifest-light.webmanifest",
          "/manifest-dark.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/img/**/*.(svg|cur|jpg|jpeg|png|apng|webp|avif|gif)",
          "/fonts/**/*.(otf|ttf|woff|woff2)",
          "/theme-light/**/*.(png|svg)",
          "/theme-dark/**/*.(png|svg)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-firestore",
      "urls": ["https://firestore.googleapis.com/**", "https://firebase.googleapis.com/**"],
      "cacheConfig": {
        "maxSize": 100,
        "maxAge": "1h",
        "strategy": "freshness"
      }
    }
  ],
  "navigationUrls": ["/**", "!/**/*.*", "!/**/*__*", "!/**/*__*/**"]
}
